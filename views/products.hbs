<div class="row">
  <div class"col-md-12" >
   <h1>End Point de productos</h1>
   <h2>{{title}}</h2>
 </div>
</div>



<div class="row">
 {{#each products}}
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{this.photo}}" class="img-responsive" alt="{{this.name}}">
      <div class="caption">
        <h3>{{this.name}}</h3>
        <p>{{this.description}}</p>
        <p>Stock: {{this.stock}}</p>
        <p><a href="#" class="btn btn-primary" role="button">Button</a> <a href="#" class="btn btn-default" role="button">Button</a></p>
      </div>
    </div>
  </div>
  {{/each}}
</div>

